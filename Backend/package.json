  },
  "homepage": "https://github.com/hsilv/CookAppAPI#readme",
  "scripts": {
    "test": "cross-env NODE_ENV=testing --env=node NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles",
